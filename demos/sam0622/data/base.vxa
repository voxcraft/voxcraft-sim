<VXA Version="1.1">
    <GPU>
        <HeapSize>0.5</HeapSize>
    </GPU>
    <Simulator>
        <SecondaryExperiment>1</SecondaryExperiment>
        <ReinitializeInitialPositionAfterThisManySeconds>5.1</ReinitializeInitialPositionAfterThisManySeconds>
        <MaxDistInVoxelLengthsToCountAsPair>3</MaxDistInVoxelLengthsToCountAsPair>
        <EnableCilia>1</EnableCilia>
        <AttachDetach>
            <EnableCollision>1</EnableCollision>
            <EnableDetach>1</EnableDetach>
            <EnableAttach>1</EnableAttach>
            <watchDistance>1.0</watchDistance>
            <boundingRadius>0.75</boundingRadius>
            <SafetyGuard>2000</SafetyGuard>
            <AttachCondition>
                <Condition_0>
                    <mtCONST>1</mtCONST>
                </Condition_0>
            </AttachCondition>
        </AttachDetach>
        <Integration>
            <DtFrac>1.0</DtFrac>
        </Integration>
        <StopCondition>
            <StopConditionFormula>
                <mtSUB>
                    <mtVAR>t</mtVAR>
                    <mtCONST>10.1</mtCONST>
                </mtSUB>
            </StopConditionFormula>
        </StopCondition>
        <Damping>
            <BondDampingZ>1</BondDampingZ>
            <ColDampingZ>0.8</ColDampingZ>
            <SlowDampingZ>0.01</SlowDampingZ>
        </Damping>
    </Simulator>
    <Environment>
        <Thermal>
            <TempPeriod>10.1</TempPeriod><!--[1/freq] stats also sampled at this freq-->
        </Thermal>
    </Environment>
    <VXC Version="0.94">
        <Lattice>
            <Lattice_Dim>0.01</Lattice_Dim>
        </Lattice>
        <Palette>
            <Material ID="1">
                <Name>Cilia</Name>
                <Display>
                    <Red>0.44</Red>
                    <Green>0.93</Green>
                    <Blue>0.72</Blue>
                    <Alpha>0.85</Alpha>
                </Display>
                <Mechanical>
                    <isMeasured>0</isMeasured>
                    <RemoveFromSimulationAfterThisManySeconds>5</RemoveFromSimulationAfterThisManySeconds>
                    <Cilia>0.09</Cilia>
                    <MatModel>0</MatModel>
                    <Elastic_Mod>9e+04</Elastic_Mod>
                    <Fail_Stress>0</Fail_Stress>
                    <Density>2e+03</Density>
                    <Poissons_Ratio>0.35</Poissons_Ratio>
                    <CTE>0</CTE>
                    <MaterialTempPhase>0</MaterialTempPhase>
                    <uStatic>1</uStatic>
                    <uDynamic>1</uDynamic>
